{
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "extends": "kellyirc",
  "rules": {
    "require-yield": "off",
    "indent": [
      "error",
      2
    ]
  },
  "parserOptions": {
    "ecmaVersion": "2018",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
